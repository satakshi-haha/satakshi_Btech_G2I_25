//Prime number.
#include <stdio.h>
int main()
{
    int i,n,c=0;
    printf("Enter the number:\t");
    scanf("%d",&n);
    
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            c++;
        }
    }
        if(c==2)
        {
        printf("The number is a prime number");
        }
        else
        {
         printf("Not a prime number");
        }
    return 0;
}
